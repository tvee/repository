<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.TVEE"
       name="TVEE"
       version="1.0.8"
       provider-name="TVEE">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.t0mm0.common" version="2.0.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.1"/>
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.beautifulsoup4" />
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.requests" />
    <import addon="script.module.httplib2" />
    <import addon="script.module.youtube.dl" optional="true"/>
    <import addon="plugin.video.youtube" version="3.0.0"/>
    <import addon="script.module.urlresolver" optional="true"/>
    <import addon="script.module.simplejson" />
    <import addon="script.common.plugin.cache" version="2.5.2"/>
    <import addon="script.module.metahandler" version="2.5.1"/>

  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">TVEE</summary>
    <description lang="en">Watch free TVEE</description>
    <platform>all</platform>
  </extension>
</addon>

    <addon id="repository.TVEE" name="TVEE" version="1.0.1" provider-name="TVEE">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="TVEE">
            <info compressed="false">https://raw.githubusercontent.com/tvee/repository/master/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/tvee/repository/master/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/tvee/repository/master</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>TVEE Repository</summary>
			<description></description>
			<disclaimer></disclaimer>
            <platform>all</platform>
    </extension>
</addon>
</addons>